{
    "name": "pxgamer/trello-php",
    "type": "library",
    "description": "A simple PHP wrapper for utilising the Trello api.",
    "license": "MIT",
    "abandoned": "stevenmaguire/trello-php",
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "pxgamer\\Trello\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pxgamer\\Trello\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}
